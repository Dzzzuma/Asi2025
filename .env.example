# Copy this file to `.env` and fill in values as needed.
# IMPORTANT: `.env` must NOT be committed to the repository.

# Path to the production model file (optional). If empty, API uses default path in code.
MODEL_PATH=

# Weights & Biases API key (optional when using local model file).
WANDB_API_KEY=

# Database connection string (SQLite by default).
DATABASE_URL=sqlite:///local.db
